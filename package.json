{
  "name": "gto",
  "version": "0.1.0",
  "private": true,
  "description": "Aokiz GTO - Game Theory Optimal Poker Training",
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:web": "pnpm --filter @gto/web dev",
    "dev:miniapp": "pnpm --filter @gto/miniapp dev:weapp",
    "build": "pnpm -r run build",
    "build:web": "pnpm --filter @gto/web build",
    "build:miniapp": "pnpm --filter @gto/miniapp build:weapp",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r run test",
    "clean": "pnpm -r run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "pg": "^8.16.3",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
