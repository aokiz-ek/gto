{
  "name": "@gto/miniapp",
  "version": "0.1.0",
  "private": true,
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@gto/core": "workspace:*",
    "@babel/runtime": "^7.21.5",
    "@tarojs/components": "3.6.x",
    "@tarojs/helper": "3.6.x",
    "@tarojs/plugin-platform-weapp": "3.6.x",
    "@tarojs/plugin-platform-alipay": "3.6.x",
    "@tarojs/plugin-platform-tt": "3.6.x",
    "@tarojs/plugin-platform-swan": "3.6.x",
    "@tarojs/plugin-platform-h5": "3.6.x",
    "@tarojs/react": "3.6.x",
    "@tarojs/runtime": "3.6.x",
    "@tarojs/shared": "3.6.x",
    "@tarojs/taro": "3.6.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "3.6.x",
    "@tarojs/webpack5-runner": "3.6.x",
    "@types/react": "^18.2.0",
    "babel-preset-taro": "3.6.x",
    "eslint": "^8.0.0",
    "sass": "^1.69.0",
    "typescript": "^5.3.0"
  }
}
